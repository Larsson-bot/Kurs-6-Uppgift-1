@page "/check"



@code {
    protected override async Task OnInitializedAsync()
    {
        var key = await sessionStorage.GetItemAsync<string>("AccessToken");
        Console.WriteLine(key);
        @if(key == null)
        {
            NavManager.NavigateTo("/login");
        }
        else
        {
            NavManager.NavigateTo("/create-customer");
        }
    }


}
